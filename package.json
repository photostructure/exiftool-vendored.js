{
  "name": "exiftool-vendored",
  "version": "18.2.0",
  "description": "Efficient, cross-platform access to ExifTool",
  "main": "./dist/ExifTool.js",
  "types": "./dist/ExifTool.d.ts",
  "homepage": "https://photostructure.github.io/exiftool-vendored.js/",
  "bugs": {
    "url": "https://github.com/photostructure/exiftool-vendored.js/issues"
  },
  "scripts": {
    "ci": "yarn install --frozen-lockfile",
    "clean": "rimraf lib dist coverage .nyc_output",
    "precompile": "yarn clean",
    "compile": "tsc",
    "premktags": "yarn compile",
    "mktags": "node dist/update/mktags.js",
    "lint": "yarn eslint src --ext .ts",
    "prettier": "prettier --write src/*.ts src/**/*.ts",
    "pretest": "run-p lint compile",
    "test": "mocha 'dist/*.spec.js'",
    "docs:1": "typedoc --options .typedoc.js",
    "docs:3": "cp .serve.json docs/serve.json",
    "docs:4": "touch docs/.nojekyll",
    "docs:5": "yarn serve docs",
    "docs": "run-s docs:*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/photostructure/exiftool-vendored.js.git"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "keywords": [
    "ExifTool",
    "EXIF",
    "JPEG",
    "ICMP",
    "TIFF",
    "RAW",
    "ARW",
    "DNG",
    "NEF",
    "ORF",
    "XMP",
    "CR2",
    "CR3",
    "MakerNote",
    "Metadata"
  ],
  "author": "Matthew McEachen <matthew-exiftoolvendored@photostructure.com>",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "**/*.spec.?s",
      "**/update/**"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/fs-extra": "^9.0.13",
    "@types/globule": "^1.1.4",
    "@types/he": "^1.1.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.20",
    "@types/progress": "^2.0.5",
    "@types/rimraf": "^3.0.2",
    "@types/tar-fs": "^2.0.1",
    "@types/tmp": "^0.2.3",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@xmldom/xmldom": "^0.8.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.1.0",
    "geo-tz": "^7.0.2",
    "globule": "^1.3.4",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "progress": "^2.0.3",
    "rimraf": "^3.0.2",
    "serve": "^14.0.1",
    "source-map-support": "^0.5.21",
    "tmp": "^0.2.1",
    "typedoc": "^0.23.15",
    "typescript": "~4.8.3",
    "xpath": "^0.0.32"
  },
  "dependencies-note": "@types/luxon is not a devDependency, as our exported TypeScript typings reference luxon types. See <https://github.com/photostructure/exiftool-vendored.js/pull/108>",
  "dependencies": {
    "@photostructure/tz-lookup": "^7.0.0",
    "@types/luxon": "^3.0.1",
    "batch-cluster": "^11.0.0",
    "he": "^1.2.0",
    "luxon": "^3.0.3"
  },
  "optionalDependencies": {
    "exiftool-vendored.exe": "12.45.0",
    "exiftool-vendored.pl": "12.45.0"
  }
}
