{
  "name": "exiftool-vendored",
  "version": "4.22.0",
  "description": "Efficient, cross-platform access to ExifTool",
  "main": "./dist/ExifTool.js",
  "types": "./dist/ExifTool.d.ts",
  "homepage": "https://exiftool-vendored.js.org/",
  "bugs": {
    "url": "https://github.com/mceachen/exiftool-vendored.js/issues"
  },
  "scripts": {
    "clean": "rimraf lib dist coverage .nyc_output",
    "precompile": "npm run clean",
    "compile": "tsc",
    "preupdate": "npm run compile",
    "update": "node dist/update/update.js",
    "premktags": "npm run compile",
    "mktags": "node dist/update/mktags.js",
    "prettier": "prettier --write src/*.ts src/**/*.ts",
    "pretest": "npm run compile",
    "test": "nyc mocha --opts .mocha.opts",
    "report": "npm run test && nyc report -r html"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mceachen/exiftool-vendored.js.git"
  },
  "engines": {
    "node": ">= 6"
  },
  "keywords": [
    "ExifTool",
    "EXIF",
    "JPEG",
    "TIFF",
    "RAW",
    "ORF",
    "DNG",
    "XMP",
    "MakerNote",
    "Metadata"
  ],
  "author": "Matthew McEachen <matthew-exiftoolvendored@mceachen.org>",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "**/*.spec.?s",
      "**/update/**"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.6",
    "@types/pify": "^3.0.1",
    "@types/rimraf": "^2.0.2",
    "@types/semver": "^5.5.0",
    "@types/tmp": "^0.0.33",
    "@types/xmldom": "^0.1.29",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cp-file": "^5.0.0",
    "decompress-zip": "^0.3.1",
    "globule": "^1.2.0",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "pify": "^3.0.0",
    "prettier": "^1.12.1",
    "progress": "^2.0.0",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "source-map-support": "^0.5.4",
    "tar-fs": "^1.16.0",
    "tmp": "^0.0.33",
    "typescript": "^2.8.3",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.27"
  },
  "dependencies": {
    "batch-cluster": "^2.1.0"
  },
  "optionalDependencies": {
    "exiftool-vendored.exe": "10.93.0",
    "exiftool-vendored.pl": "10.93.0"
  }
}
